<div class="wrapper">
  <%= form_for [@photo] do |f| %>

    <div class="file-upload">
      <%= f.file_field :photos, multiple: true %>
      <i class="fa fa-arrow-up"></i>
    </div>

    <div class='text-center button-container'>
      <%= f.submit "Upload", class: 'button' %>
    </div>

  <% end %>
</div>
